            <header class="main-box-header clearfix">                
                <div class="pull-left">
                    <label>Nomor LPO / SBBK : </label>
                    <com:TTextBox ID="txtNoLPOSBBKBaru" CssClass="form-control" Attributes.autofocus="true" />
                    <com:TRequiredFieldValidator ValidationGroup="formNomorSBBK1" Display="Dynamic" ControlToValidate="txtNoLPOSBBKBaru" ErrorMessage="Isi Nomor LPO atau SBBK" CssClass="has-error help-block" />	
                    <com:TActiveCustomValidator ControlToValidate="txtNoLPOSBBKBaru" Display="Dynamic" OnServerValidate="checkBuatSBBKBaru" ValidationGroup="formNomorSBBK1" ErrorMessage="Nomor SBBK Yang Anda Inputkan Sudah Ada" FocusOnError="true" CssClass="has-error help-block" />                    
                    <com:TRequiredFieldValidator ValidationGroup="formNomorSBBK2" Display="Dynamic" ControlToValidate="txtNoLPOSBBKBaru" ErrorMessage="Isi Nomor LPO atau SBBK" CssClass="has-error help-block" />	
                    <com:TActiveCustomValidator ControlToValidate="txtNoLPOSBBKBaru" Display="Dynamic" OnServerValidate="checkUbahSBBKBaru" ValidationGroup="formNomorSBBK2" ErrorMessage="Nomor SBBK Yang Anda Inputkan Sudah Ada" FocusOnError="true" CssClass="has-error help-block" />                    
