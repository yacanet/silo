                        <prop:ClientSide.OnLoading>
                            $('<%=$this->btnUbahSBBK->ClientId%>').disabled='disabled';						
                        </prop:ClientSide.OnLoading>
                        <prop:ClientSide.OnComplete>																	                                    						                                                                            
                            $('<%=$this->btnUbahSBBK->ClientId%>').disabled='';
                            $('loading').hide(); 
                        </prop:ClientSide.OnComplete>
                    </com:TActiveLinkButton>                         
                    <com:TActiveLinkButton ID="btnBuatSBBK" CssClass="btn btn-primary pull-right" OnClick="buatSBBK" ValidationGroup="formNomorSBBK1">
