<%@ Title="Permintaan - LPO Baru" %>
<com:TContent ID="moduleheader">
    <i class="fa fa-file"></i> Lembar Permintaan Obat (LPO) Baru
</com:TContent>
<com:TContent ID="modulebreadcrumb">    
    <li><a href="#">Permintaan</a></li>
    <li class="active"><span>LPO Baru</span></li>
</com:TContent>
<com:TContent ID="csscontent">
<link rel="stylesheet" type="text/css" href="<%=$this->Theme->baseUrl%>/css/libs/nifty-component.css"/>
</com:TContent>
<com:TContent ID="maincontent">
<com:TActiveHiddenField ID="hiddenidobat" Visible="false" />
<com:TPanel Visible="<%= $this->getAddProcess(false) %>">
    <com:THiddenField ID="hiddenno_lpo" />
    <div class="row">
        <div class="col-lg-12">
            <div class="main-box clearfix">
                <header class="main-box-header clearfix">
                    <h2 class="pull-left">No. LPO : <%=$this->datalpo['no_lpo']%></h2>
                    <div class="filter-block pull-right">                                                
                        <com:TActiveLinkButton ID="btnCloseBuat" CssClass="btn btn-warning pull-right" OnClick="closeLPO" ValidationGroup="formNomorLPO2">
                            <prop:Text><i class="fa fa-times fa-lg"></i></prop:Text>
                            <prop:ClientSide.OnPreDispatch>                                                                   
                                $('loading').show(); 
                                $('<%=$this->btnCloseBuat->ClientId%>').disabled='disabled';						
                            </prop:ClientSide.OnPreDispatch>
                            <prop:ClientSide.OnLoading>
                                $('<%=$this->btnCloseBuat->ClientId%>').disabled='disabled';						
                            </prop:ClientSide.OnLoading>
                            <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                $('<%=$this->btnCloseBuat->ClientId%>').disabled='';
                                $('loading').hide(); 
                            </prop:ClientSide.OnComplete>
                        </com:TActiveLinkButton>
                    </div>
                </header>
                <div class="main-box-body clearfix">
                    <div class="form-horizontal">                                                        
                        <div class="form-group">
                            <label class="col-lg-2 control-label">Tanggal LPO :</label>
                            <div class="col-lg-10">
                                <com:TActiveCustomDatePicker ID="cmbAddTanggalLPO" DateFormat="dd-MMMM-yyyy" Culture="id"  FromYear="<%=$_SESSION['ta']%>" UpToYear="<%=$_SESSION['ta']%>" InputMode="DropDownList" OnCallback="changeTanggalLPO">                                                                        
                                    <prop:ClientSide.OnPreDispatch>                                                                   
                                        $('loading').show(); 
                                        $('<%=$this->cmbAddTanggalLPO->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnPreDispatch>
                                    <prop:ClientSide.OnLoading>
                                        $('<%=$this->cmbAddTanggalLPO->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnLoading>
                                    <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                        $('<%=$this->cmbAddTanggalLPO->ClientId%>').disabled='';
                                        $('loading').hide(); 
                                    </prop:ClientSide.OnComplete>
                                </com:TActiveCustomDatePicker>
                            </div>
                        </div>                                                                    
                        <div class="form-group" id="divAddNoLPO">
                            <label class="col-lg-2 control-label">No. LPO :</label>
                            <div class="col-lg-10">
                                <div class="row">
                                    <div class="col-lg-3 col-sm-6">
                                        <com:TTextBox ID="txtAddNoLPO" CssClass="form-control" /> 
                                    </div>
                                    <div class="col-lg-4 col-sm-6">
                                        <com:TRequiredFieldValidator ID="FieldAddNoLPOValidator" ValidationGroup="addlpo" Display="Dynamic" ControlToValidate="txtAddNoLPO" ErrorMessage="Isi No. LPO" CssClass="has-error help-block">	
                                            <prop:ClientSide.OnValidationError>
                                                $('divAddNoLPO').addClassName('has-error');
                                                jQuery('#<%=$this->FieldAddNoLPOValidator->ClientID%>').removeAttr('style');
                                            </prop:ClientSide.OnValidationError>
                                            <prop:ClientSide.OnValidationSuccess>
                                                $('divAddNoLPO').removeClassName('has-error');
                                            </prop:ClientSide.OnValidationSuccess>
                                        </com:TRequiredFieldValidator>     
                                        <com:TActiveCustomValidator ID="addNoLPO" ControlToValidate="txtAddNoLPO" Display="Dynamic" OnServerValidate="checkNoLPO" ValidationGroup="addlpo" ErrorMessage="Nomor LPO Yang Anda Inputkan Sudah Ada" FocusOnError="true" CssClass="has-error help-block">
                                            <prop:ClientSide.OnValidationError>
                                                $('divAddNoLPO').addClassName('has-error');
                                                jQuery('#<%=$this->addNoLPO->ClientID%>').removeAttr('style');
                                            </prop:ClientSide.OnValidationError>
                                            <prop:ClientSide.OnValidationSuccess>
                                                $('divAddNoLPO').removeClassName('has-error');
                                            </prop:ClientSide.OnValidationSuccess>
                                        </com:TActiveCustomValidator>
                                    </div>
                                </div>
                            </div>
                        </div>                         
                        <div class="form-group" id="divTxtAddJumlahKunjunganBayar">
                            <label class="col-lg-2 control-label">Jml. Kunjungan Bayar :</label>
                            <div class="col-lg-2">
                                <com:TTextBox ID="txtAddJumlahKunjunganBayar" CssClass="form-control" Attributes.OnKeyUp="formatangka(this,true)" />                                
                                <com:TRequiredFieldValidator ID="FieldAddJumlahKunjunganBayarValidator" ValidationGroup="addlpo" Display="Dynamic" ControlToValidate="txtAddJumlahKunjunganBayar" ErrorMessage="Isi jumlah kunjungan Bayar" CssClass="has-error help-block">	
                                    <prop:ClientSide.OnValidationError>
                                        $('divTxtAddJumlahKunjunganBayar').addClassName('has-error');
                                        jQuery('#<%=$this->FieldAddJumlahKunjunganBayarValidator->ClientID%>').removeAttr('style');
                                    </prop:ClientSide.OnValidationError>
                                    <prop:ClientSide.OnValidationSuccess>
                                        $('divTxtAddJumlahKunjunganBayar').removeClassName('has-error');
                                    </prop:ClientSide.OnValidationSuccess>
                                </com:TRequiredFieldValidator>     
                            </div>
                        </div> 
                        <div class="form-group" id="divTxtAddJumlahKunjunganGratis">
                            <label class="col-lg-2 control-label">Jml. Kunjungan Gratis :</label>
                            <div class="col-lg-2">
                                <com:TTextBox ID="txtAddJumlahKunjunganGratis" CssClass="form-control" Attributes.OnKeyUp="formatangka(this,true)" />                                
                                <com:TRequiredFieldValidator ID="FieldAddJumlahKunjunganGratisValidator" ValidationGroup="addlpo" Display="Dynamic" ControlToValidate="txtAddJumlahKunjunganGratis" ErrorMessage="Isi jumlah kunjungan Gratis" CssClass="has-error help-block">	
                                    <prop:ClientSide.OnValidationError>
                                        $('divTxtAddJumlahKunjunganGratis').addClassName('has-error');
                                        jQuery('#<%=$this->FieldAddJumlahKunjunganGratisValidator->ClientID%>').removeAttr('style');
                                    </prop:ClientSide.OnValidationError>
                                    <prop:ClientSide.OnValidationSuccess>
                                        $('divTxtAddJumlahKunjunganGratis').removeClassName('has-error');
                                    </prop:ClientSide.OnValidationSuccess>
                                </com:TRequiredFieldValidator>     
                            </div>
                        </div> 
                        <div class="form-group" id="divTxtAddJumlahKunjunganBPJS">
                            <label class="col-lg-2 control-label">Jml. Kunjungan BPJS :</label>
                            <div class="col-lg-2">
                                <com:TTextBox ID="txtAddJumlahKunjunganBPJS" CssClass="form-control" Attributes.OnKeyUp="formatangka(this,true)" />                                
                                <com:TRequiredFieldValidator ID="FieldAddJumlahKunjunganBPJSValidator" ValidationGroup="addlpo" Display="Dynamic" ControlToValidate="txtAddJumlahKunjunganBPJS" ErrorMessage="Isi jumlah kunjungan BPJS" CssClass="has-error help-block">	
                                    <prop:ClientSide.OnValidationError>
                                        $('divTxtAddJumlahKunjunganBPJS').addClassName('has-error');
                                        jQuery('#<%=$this->FieldAddJumlahKunjunganBPJSValidator->ClientID%>').removeAttr('style');
                                    </prop:ClientSide.OnValidationError>
                                    <prop:ClientSide.OnValidationSuccess>
                                        $('divTxtAddJumlahKunjunganBPJS').removeClassName('has-error');
                                    </prop:ClientSide.OnValidationSuccess>
                                </com:TRequiredFieldValidator>     
                            </div>
                        </div> 
                        <div class="form-group" id="divTxtAddNIPKA">
                            <label class="col-lg-2 control-label">NIP Kepala Puskesmas :</label>
                            <div class="col-lg-3">
                                <com:TTextBox ID="txtAddNIPKA" CssClass="form-control" Width="180px" ReadOnly="true"/>                                
                                <com:TRequiredFieldValidator ID="FieldAddNIPKAValidator" ValidationGroup="addlpo" Display="Dynamic" ControlToValidate="txtAddNIPKA" ErrorMessage="NIP Kepala Puskesmas belum ada, hubungi superadmin." CssClass="has-error help-block">	
                                    <prop:ClientSide.OnValidationError>
                                        $('divTxtAddNIPKA').addClassName('has-error');
                                        jQuery('#<%=$this->FieldAddNIPKAValidator->ClientID%>').removeAttr('style');
                                    </prop:ClientSide.OnValidationError>
                                    <prop:ClientSide.OnValidationSuccess>
                                        $('divTxtAddNIPKA').removeClassName('has-error');
                                    </prop:ClientSide.OnValidationSuccess>
                                </com:TRequiredFieldValidator>     
                            </div>
                        </div> 
                        <div class="form-group" id="divTxtAddNamaKA">
                            <label class="col-lg-2 control-label">Nama Kepala Puskesmas :</label>
                            <div class="col-lg-4">
                                <com:TTextBox ID="txtAddNamaKA" CssClass="form-control" ReadOnly="true" />                                
                                <com:TRequiredFieldValidator ID="FieldAddNamaKAValidator" ValidationGroup="addlpo" Display="Dynamic" ControlToValidate="txtAddNamaKA" ErrorMessage="Nama Kepala Puskesmas belum ada, hubungi superadmin." CssClass="has-error help-block">	
                                    <prop:ClientSide.OnValidationError>
                                        $('divTxtAddNamaKA').addClassName('has-error');
                                        jQuery('#<%=$this->FieldAddNamaKAValidator->ClientID%>').removeAttr('style');
                                    </prop:ClientSide.OnValidationError>
                                    <prop:ClientSide.OnValidationSuccess>
                                        $('divTxtAddNamaKA').removeClassName('has-error');
                                    </prop:ClientSide.OnValidationSuccess>
                                </com:TRequiredFieldValidator>     
                            </div>
                        </div>
                        <div class="form-group" id="divTxtAddNIPPengelola">
                            <label class="col-lg-2 control-label">NIP Pengelola Obat :</label>
                            <div class="col-lg-3">
                                <com:TTextBox ID="txtAddNIPPengelola" CssClass="form-control" Width="180px" ReadOnly="true"/>                                
                                <com:TRequiredFieldValidator ID="FieldAddNIPPengelolaValidator" ValidationGroup="addlpo" Display="Dynamic" ControlToValidate="txtAddNIPPengelola" ErrorMessage="NIP Petugas Pengelola Obat belum ada, hubungi superadmin." CssClass="has-error help-block">	
                                    <prop:ClientSide.OnValidationError>
                                        $('divTxtAddNIPPengelola').addClassName('has-error');
                                        jQuery('#<%=$this->FieldAddNIPPengelolaValidator->ClientID%>').removeAttr('style');
                                    </prop:ClientSide.OnValidationError>
                                    <prop:ClientSide.OnValidationSuccess>
                                        $('divTxtAddNIPPengelola').removeClassName('has-error');
                                    </prop:ClientSide.OnValidationSuccess>
                                </com:TRequiredFieldValidator>     
                            </div>
                        </div> 
                        <div class="form-group" id="divTxtAddNamaPengelola">
                            <label class="col-lg-2 control-label">Nama Pengelola Obat :</label>
                            <div class="col-lg-4">
                                <com:TTextBox ID="txtAddNamaPengelola" CssClass="form-control" ReadOnly="true" />                                
                                <com:TRequiredFieldValidator ID="FieldAddNamaPengelolaValidator" ValidationGroup="addlpo" Display="Dynamic" ControlToValidate="txtAddNamaPengelola" ErrorMessage="Nama Petugas Pengelola Obat belum ada, hubungi superadmin." CssClass="has-error help-block">	
                                    <prop:ClientSide.OnValidationError>
                                        $('divTxtAddNamaPengelola').addClassName('has-error');
                                        jQuery('#<%=$this->FieldAddNamaPengelolaValidator->ClientID%>').removeAttr('style');
                                    </prop:ClientSide.OnValidationError>
                                    <prop:ClientSide.OnValidationSuccess>
                                        $('divTxtAddNamaPengelola').removeClassName('has-error');
                                    </prop:ClientSide.OnValidationSuccess>
                                </com:TRequiredFieldValidator>     
                            </div>
                        </div>                        
                        <div class="form-group" id="divAddTotalQTY">
                            <label class="col-lg-2 control-label">Total QTY Permintaan :</label>
                            <div class="col-lg-10">
                                <div class="row">
                                    <div class="col-lg-1 col-sm-11">
                                        <p class="form-control-static"><%=LPOBaru::$totalQTY%></p>
                                        <com:THiddenField ID="hiddenTotalQTY" Value="<%=LPOBaru::$totalQTY%>" />
                                    </div>                                    
                                </div>
                            </div>
                        </div>                     
                        <div class="form-group">
                            <div class="col-lg-offset-2 col-lg-10">
                                <com:TActiveCustomButton ID="btnSave" CssClass="btn btn-primary" OnClick="saveData" ValidationGroup="addlpo">
                                    <prop:Text><i class="fa fa-save"></i> Simpan</prop:Text>
                                    <prop:ClientSide.OnPreDispatch>                                                                   
                                        $('loading').show(); 
                                        $('<%=$this->btnSave->ClientId%>').disabled='disabled';						
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='disabled';
                                    </prop:ClientSide.OnPreDispatch>
                                    <prop:ClientSide.OnLoading>
                                        $('<%=$this->btnSave->ClientId%>').disabled='disabled';						
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='disabled';
                                    </prop:ClientSide.OnLoading>
                                    <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                        $('<%=$this->btnSave->ClientId%>').disabled='';
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='';
                                        $('loading').hide(); 
                                    </prop:ClientSide.OnComplete>
                                </com:TActiveCustomButton>
                                <com:TActiveCustomButton ID="btnCheckout" CssClass="btn btn-success" OnClick="checkOut" ValidationGroup="addlpo">
                                    <prop:Text><i class="fa fa-shopping-cart"></i> Checkout</prop:Text>
                                    <prop:ClientSide.OnPreDispatch>                                                                   
                                        $('loading').show(); 
                                        $('<%=$this->btnSave->ClientId%>').disabled='disabled';
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnPreDispatch>
                                    <prop:ClientSide.OnLoading>
                                        $('<%=$this->btnSave->ClientId%>').disabled='disabled';
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnLoading>
                                    <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                        $('<%=$this->btnSave->ClientId%>').disabled='';
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='';
                                        $('loading').hide(); 
                                    </prop:ClientSide.OnComplete>
                                </com:TActiveCustomButton>
                                <com:TActiveCustomButton ID="btnBatal" CssClass="btn btn-trash btn-danger" OnClick="batalLPO" Attributes.OnClick="if(!confirm('Anda ingin membatalkan (menghapus) data LPO ini ?')) return false;">
                                    <prop:Text><i class="fa fa-times"></i> Batal</prop:Text>
                                    <prop:ClientSide.OnPreDispatch>                                                                   
                                        $('loading').show(); 
                                        $('<%=$this->btnBatal->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnPreDispatch>
                                    <prop:ClientSide.OnLoading>
                                        $('<%=$this->btnBatal->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnLoading>
                                    <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                        $('<%=$this->btnBatal->ClientId%>').disabled='';
                                        $('loading').hide(); 
                                    </prop:ClientSide.OnComplete>
                                </com:TActiveCustomButton>
                            </div>
                        </div>                    
                    </div>                
                </div>
            </div>
        </div>
    </div>    
    <div class="row">
        <div class="col-lg-12">
            <div class="main-box clearfix">
                <header class="main-box-header clearfix">
                    <h2 class="pull-left"><i class="fa fa-shopping-cart"></i> Keranjang Permintaan Obat di Bulan <com:TActiveLabel ID="labelBulanPermintaanObat" /> untuk Bulan Setelahnya</h2>
                    <div class="filter-block pull-right">                                                
                        <com:TActiveLinkButton ID="btnClear" CssClass="btn btn-warning pull-right" OnClick="clearCart" ValidationGroup="formNomorLPO2">
                            <prop:Text><i class="fa fa-trash-o fa-lg"></i> Kosongkan</prop:Text>
                            <prop:ClientSide.OnPreDispatch>                                                                   
                                $('loading').show(); 
                                $('<%=$this->btnClear->ClientId%>').disabled='disabled';						
                            </prop:ClientSide.OnPreDispatch>
                            <prop:ClientSide.OnLoading>
                                $('<%=$this->btnClear->ClientId%>').disabled='disabled';						
                            </prop:ClientSide.OnLoading>
                            <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                $('<%=$this->btnClear->ClientId%>').disabled='';
                                $('loading').hide(); 
                            </prop:ClientSide.OnComplete>
                        </com:TActiveLinkButton>
                    </div>
                </header>
                <div class="main-box-body clearfix">
                    <com:TActiveRepeater ID="RepeaterCart" DataKeyField="iddetail_lpo" OnItemCreated="itemCreatedCart">
                        <prop:HeaderTemplate>			
                            <table class="table table-striped table-hover">	
                                <thead>
                                <tr>                                        
                                    <th width="120"><span>KODE OBAT</span></th>
                                    <th width="250"><span>NAMA OBAT</span></th>                                        
                                    <th width="100"><span>KEMASAN</span></th>                                        
                                    <th><span>HARGA</span></th>
                                    <th><span>STOCK AWAL</span></th>
                                    <th><span>PENERIMAAN</span></th>
                                    <th><span>PERSEDIAAN</span></th>
                                    <th><span>PEMAKAIAN</span></th>
                                    <th><span>STOCK AKHIR</span></th>
                                    <th><span>PERMINTAAN</span></th>
                                    <th width="150"></th>							
                                </tr>								
                                </thead>
                                <tbody>
                        </prop:HeaderTemplate>
                        <prop:ItemTemplate>
                            <tr>                                    
                                <td><%#$this->DataItem['kode_obat']%></td>
                                <td><%#$this->DataItem['nama_obat']%></td>                                       
                                <td><%#$this->DataItem['kemasan']%></td>                                    
                                <td><%#$this->Page->Obat->toRupiah($this->DataItem['harga'])%></td>    
                                <td><%#$this->DataItem['awal_stock']%></td>                                    
                                <td><%#$this->DataItem['total_penerimaan']%></td>
                                <td><%#$this->DataItem['persediaan']%></td>
                                <td><%#$this->DataItem['total_pemakaian']%></td>
                                <td><%#$this->DataItem['stock_akhir']%></td>
                                <td><com:TTextBox ID="txtQTY" CssClass="form-control" Attributes.OnKeyUp="formatangka(this,true)" Text="<%#$this->Page->getQTYFromCart($this->DataItem['iddetail_lpo'])%>" Width="60px" /></td>
                                <td>                                                                          
                                    <com:TActiveLinkButton ID="btnSimpanQtyRepeaterCart" CssClass="table-link" OnClick="Page.addProductRepeater" Attributes.Title="simpan" CommandParameter="<%#$this->DataItem['idobat_puskesmas']%>">
                                        <prop:Text>
                                            <span class="fa-stack">
                                                <i class="fa fa-square fa-stack-2x"></i>
                                                <i class="fa fa-save fa-stack-1x fa-inverse"></i>
                                            </span>
                                        </prop:Text>
                                        <prop:ClientSide.OnPreDispatch>
                                            $('loading').show();                                             
                                            $('<%=$this->btnSimpanQtyRepeaterCart->ClientId%>').disabled='disabled';						
                                        </prop:ClientSide.OnPreDispatch>
                                        <prop:ClientSide.OnLoading>
                                            $('<%=$this->btnSimpanQtyRepeaterCart->ClientId%>').disabled='disabled';									                            
                                        </prop:ClientSide.OnLoading>
                                        <prop:ClientSide.onComplete>
                                            $('loading').hide();                                                
                                        </prop:ClientSide.OnComplete>
                                    </com:TActiveLinkButton>                                        
                                    <com:TActiveLinkButton ID="btnDelete" ClientSide.PostState="false" CssClass="table-link danger" OnClick="Page.deleteItem" Attributes.Title="Hapus Item" Attributes.OnClick="if(!confirm('Anda ingin menghapus item ini ?')) return false;">
                                        <prop:Text>
                                            <span class="fa-stack">
                                                <i class="fa fa-square fa-stack-2x"></i>
                                                <i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
                                            </span>
                                        </prop:Text>
                                        <prop:ClientSide.OnPreDispatch>
                                            $('loading').show();                                             
                                            $('<%=$this->btnDelete->ClientId%>').disabled='disabled';						
                                        </prop:ClientSide.OnPreDispatch>
                                        <prop:ClientSide.OnLoading>
                                            $('<%=$this->btnDelete->ClientId%>').disabled='disabled';									                            
                                        </prop:ClientSide.OnLoading>
                                        <prop:ClientSide.onComplete>
                                            $('loading').hide();                                                
                                        </prop:ClientSide.OnComplete>
                                    </com:TActiveLinkButton>  
                                </td>                                    
                            </tr>
                        </prop:ItemTemplate>				                
                        <prop:FooterTemplate>               
                            </tbody>
                            </table>
                            <div class="invoice-box-total clearfix">
                                <div class="row">
                                    <div class="col-sm-9 col-md-10 col-xs-6 text-right invoice-box-total-label">
                                        Total QTY : <%=LPOBaru::$totalQTY%>
                                    </div>                                    
                                </div>
                                <div class="row">                                    
                                    <div class="col-sm-9 col-md-10 col-xs-6 text-right invoice-box-total-label">
                                        Total Harga : <%=$this->Page->Obat->toRupiah(LPOBaru::$totalHARGA)%>
                                    </div>                                    
                                </div>
                            </div>
                        </prop:FooterTemplate>	
                        <prop:EmptyTemplate>                                                       
                            <div class="alert alert-info">
                                <i class="fa fa-info-circle fa-fw fa-lg"></i>
                                <strong>
                                    Info!
                                </strong>
                                 Data Obat untuk LPO belum ada.
                            </div>
                        </prop:EmptyTemplate>
                    </com:TActiveRepeater>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="main-box clearfix">
                <header class="main-box-header clearfix">                    
                    <h2 class="pull-left"><i class="fa fa-medkit"></i> Daftar Penggunaan Obat Bulan <com:TActiveLabel ID="labelBulanPenggunaanObat" /></h2>                    
                    <div class="filter-block pull-right">
                        <com:TActiveCustomButton ID="btnAddAll" CssClass="btn btn-primary" OnClick="addAllProductRepeater" ValidationGroup="addpemakaian">
                            <prop:Text><i class="fa fa-plus"></i> Tambah Semua</prop:Text>
                            <prop:ClientSide.OnPreDispatch>                                                                   
                                $('loading').show();                                 
                                $('<%=$this->btnAddAll->ClientId%>').disabled='disabled';						
                            </prop:ClientSide.OnPreDispatch>
                            <prop:ClientSide.OnLoading>                                
                                $('<%=$this->btnAddAll->ClientId%>').disabled='disabled';						
                            </prop:ClientSide.OnLoading>
                            <prop:ClientSide.OnComplete>                                
                                $('<%=$this->btnAddAll->ClientId%>').disabled='';
                                $('loading').hide(); 
                            </prop:ClientSide.OnComplete>
                        </com:TActiveCustomButton>
                    </div>
                </header>
                <div class="main-box-body clearfix">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-lg-2 control-label">Kriteria :</label>
                            <div class="col-lg-2">
                                <com:TDropDownList ID="cmbKriteria" CssClass="form-control">                                                           
                                    <com:TListItem Text="Kode Obat" Value="kode" />
                                    <com:TListItem Text="Nama Obat" Value="nama" />
                                </com:TDropDownList>
                            </div>
                        </div>
                        <div class="form-group" id="divtxtKriteria">
                            <label class="col-lg-2 control-label">Isi Kriteria :</label>
