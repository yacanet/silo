                                    <prop:ClientSide.OnValidationError>
                                        $('divAddSumberDana').addClassName('has-error');
                                        jQuery('#<%=$this->FieldAddSumberDanaValidator->ClientID%>').removeAttr('style');
                                    </prop:ClientSide.OnValidationError>
                                    <prop:ClientSide.OnValidationSuccess>
                                        $('divAddSumberDana').removeClassName('has-error');
                                    </prop:ClientSide.OnValidationSuccess>
                                </com:TRequiredFieldValidator>    
                            </div>
                        </div>                                                               
                        <div class="form-group" id="divAddProgram">
                            <label class="col-lg-2 control-label">Obat Program :</label>
                            <div class="col-lg-3">
                                <com:TDropDownList ID="cmbAddProgram" CssClass="form-control" />                                                                    
                                <com:TRequiredFieldValidator ID="FieldAddProgramValidator" InitialValue="none" ValidationGroup="addsbbm" Display="Dynamic" ControlToValidate="cmbAddProgram" ErrorMessage="Pilih Program Obat" CssClass="has-error help-block">	
                                    <prop:ClientSide.OnValidationError>
                                        $('divAddProgram').addClassName('has-error');
                                        jQuery('#<%=$this->FieldAddProgramValidator->ClientID%>').removeAttr('style');
                                    </prop:ClientSide.OnValidationError>
                                    <prop:ClientSide.OnValidationSuccess>
                                        $('divAddProgram').removeClassName('has-error');
                                    </prop:ClientSide.OnValidationSuccess>
                                </com:TRequiredFieldValidator>    
                            </div>
                        </div>                                                               
                        <div class="form-group form-group-select2" id="divAddPenyalur">
                            <label class="col-lg-2 control-label">Penyalur :</label>
                            <div class="col-lg-4">
                                <com:TDropDownList ID="cmbAddPenyalur" Width="300px" />
                                <com:TRequiredFieldValidator ID="FieldAddPenyalurValidator" InitialValue="none" ValidationGroup="addsbbm" Display="Dynamic" ControlToValidate="cmbAddPenyalur" ErrorMessage="Pilih Bentuk Sediaan Obat" CssClass="has-error help-block">	
                                    <prop:ClientSide.OnValidationError>
                                        $('divAddPenyalur').addClassName('has-error');
                                        jQuery('#<%=$this->FieldAddPenyalurValidator->ClientID%>').removeAttr('style');
                                    </prop:ClientSide.OnValidationError>
                                    <prop:ClientSide.OnValidationSuccess>
                                        $('divAddPenyalur').removeClassName('has-error');
                                    </prop:ClientSide.OnValidationSuccess>
                                </com:TRequiredFieldValidator>    
                            </div>
                        </div>                                                
                        <div class="form-group" id="divAddNoFaktur">
                            <label class="col-lg-2 control-label">No. Faktur :</label>
                            <div class="col-lg-10">
                                <div class="row">
                                    <div class="col-lg-3 col-sm-6">
                                        <com:TTextBox ID="txtAddNoFaktur" CssClass="form-control" /> 
                                    </div>
                                    <div class="col-lg-4 col-sm-6">
                                        <com:TRequiredFieldValidator ID="FieldAddNoFakturValidator" ValidationGroup="addsbbm" Display="Dynamic" ControlToValidate="txtAddNoFaktur" ErrorMessage="Isi No. Faktur" CssClass="has-error help-block">	
                                            <prop:ClientSide.OnValidationError>
                                                $('divAddNoFaktur').addClassName('has-error');
                                                jQuery('#<%=$this->FieldAddNoFakturValidator->ClientID%>').removeAttr('style');
                                            </prop:ClientSide.OnValidationError>
                                            <prop:ClientSide.OnValidationSuccess>
                                                $('divAddNoFaktur').removeClassName('has-error');
                                            </prop:ClientSide.OnValidationSuccess>
                                        </com:TRequiredFieldValidator>     
                                        <com:TActiveCustomValidator ID="addNoFaktur" ControlToValidate="txtAddNoFaktur" Display="Dynamic" OnServerValidate="checkNoFaktur" ValidationGroup="addsbbm" ErrorMessage="Nomor Faktur Yang Anda Inputkan Sudah Ada" FocusOnError="true" CssClass="has-error help-block">
                                            <prop:ClientSide.OnValidationError>
                                                $('divAddNoFaktur').addClassName('has-error');
                                                jQuery('#<%=$this->addNoFaktur->ClientID%>').removeAttr('style');
                                            </prop:ClientSide.OnValidationError>
                                            <prop:ClientSide.OnValidationSuccess>
                                                $('divAddNoFaktur').removeClassName('has-error');
                                            </prop:ClientSide.OnValidationSuccess>
                                        </com:TActiveCustomValidator>
                                    </div>
                                </div>
                            </div>
                        </div> 
                        <div class="form-group">
                            <label class="col-lg-2 control-label">Tanggal Faktur :</label>
                            <div class="col-lg-10">
                                <com:TCustomDatePicker ID="cmbAddTanggalFaktur" DateFormat="dd-MMMM-yyyy" Culture="id"  FromYear="<%=$_SESSION['ta']%>" InputMode="DropDownList" />
                            </div>
                        </div>
                        <div class="form-group" id="divAddPenerima">
                            <label class="col-lg-2 control-label">Penerima :</label>
                            <div class="col-lg-10">
                                <div class="row">
                                    <div class="col-lg-6 col-sm-6">
                                        <com:TTextBox ID="txtAddPenerima" CssClass="form-control" /> 
                                    </div>
                                    <div class="col-lg-4 col-sm-6">
                                        <com:TRequiredFieldValidator ID="FieldAddPenerimaValidator" ValidationGroup="addsbbm" Display="Dynamic" ControlToValidate="txtAddPenerima" ErrorMessage="Isi Penerima" CssClass="has-error help-block">	
                                            <prop:ClientSide.OnValidationError>
                                                $('divAddPenerima').addClassName('has-error');
                                                jQuery('#<%=$this->FieldAddPenerimaValidator->ClientID%>').removeAttr('style');
                                            </prop:ClientSide.OnValidationError>
                                            <prop:ClientSide.OnValidationSuccess>
                                                $('divAddPenerima').removeClassName('has-error');
                                            </prop:ClientSide.OnValidationSuccess>
                                        </com:TRequiredFieldValidator>     
                                    </div>
                                </div>
                            </div>
                        </div>   
                        <div class="form-group" id="divAddTotalQTY">
                            <label class="col-lg-2 control-label">Total QTY :</label>
                            <div class="col-lg-10">
                                <div class="row">
                                    <div class="col-lg-1 col-sm-11">
                                        <p class="form-control-static"><%=SBBMBaru::$totalQTY%></p>
                                        <com:THiddenField ID="hiddenTotalQTY" Value="<%=SBBMBaru::$totalQTY%>" />
                                    </div>
                                    <div class="col-lg-10 col-sm-2">
                                        <com:TCompareValidator ValidationGroup="addsbbm" ControlToValidate="hiddenTotalQTY" ValueToCompare="0" DataType="Integer" Operator="GreaterThan" Text="Jumlah Quantity keranjang obat harus lebih besar dari 0." />
                                    </div>
                                </div>
                            </div>
                        </div>                     
                        <div class="form-group">
                            <div class="col-lg-offset-2 col-lg-10">
                                <com:TActiveCustomButton ID="btnSave" CssClass="btn btn-primary" OnClick="saveData" ValidationGroup="addsbbm">
                                    <prop:Text><i class="fa fa-save"></i> Simpan</prop:Text>
                                    <prop:ClientSide.OnPreDispatch>                                                                   
                                        $('loading').show(); 
                                        $('<%=$this->btnSave->ClientId%>').disabled='disabled';						
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='disabled';
                                    </prop:ClientSide.OnPreDispatch>
                                    <prop:ClientSide.OnLoading>
                                        $('<%=$this->btnSave->ClientId%>').disabled='disabled';						
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='disabled';
                                    </prop:ClientSide.OnLoading>
                                    <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                        $('<%=$this->btnSave->ClientId%>').disabled='';
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='';
                                        $('loading').hide(); 
                                    </prop:ClientSide.OnComplete>
                                </com:TActiveCustomButton>
                                <com:TActiveCustomButton ID="btnCheckout" CssClass="btn btn-success" OnClick="checkOut" ValidationGroup="addsbbm">
                                    <prop:Text><i class="fa fa-shopping-cart"></i> Checkout</prop:Text>
                                    <prop:ClientSide.OnPreDispatch>                                                                   
                                        $('loading').show(); 
                                        $('<%=$this->btnSave->ClientId%>').disabled='disabled';
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnPreDispatch>
                                    <prop:ClientSide.OnLoading>
                                        $('<%=$this->btnSave->ClientId%>').disabled='disabled';
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnLoading>
                                    <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                        $('<%=$this->btnSave->ClientId%>').disabled='';
                                        $('<%=$this->btnCheckout->ClientId%>').disabled='';
                                        $('loading').hide(); 
                                    </prop:ClientSide.OnComplete>
                                </com:TActiveCustomButton>
                                <com:TActiveCustomButton ID="btnBatal" CssClass="btn btn-trash btn-danger" OnClick="batalSBBM" Attributes.OnClick="if(!confirm('Anda ingin membatalkan (menghapus) data SBBM ini ?')) return false;">
                                    <prop:Text><i class="fa fa-times"></i> Batal</prop:Text>
                                    <prop:ClientSide.OnPreDispatch>                                                                   
                                        $('loading').show(); 
                                        $('<%=$this->btnBatal->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnPreDispatch>
                                    <prop:ClientSide.OnLoading>
                                        $('<%=$this->btnBatal->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnLoading>
                                    <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                        $('<%=$this->btnBatal->ClientId%>').disabled='';
                                        $('loading').hide(); 
                                    </prop:ClientSide.OnComplete>
                                </com:TActiveCustomButton>
                            </div>
                        </div>                    
                    </div>                
                </div>
            </div>
        </div>
    </div>    
    <div class="row">
        <div class="col-lg-12">
            <div class="main-box clearfix">
                <header class="main-box-header clearfix">
                    <h2 class="pull-left"><i class="fa fa-shopping-cart"></i> Keranjang Obat dari Penyalur</h2>
                    <div class="filter-block pull-right">                                                
                        <com:TActiveLinkButton ID="btnClear" CssClass="btn btn-warning pull-right" OnClick="clearCart" ValidationGroup="formNomorSBBM2">
                            <prop:Text><i class="fa fa-trash-o fa-lg"></i> Kosongkan</prop:Text>
                            <prop:ClientSide.OnPreDispatch>                                                                   
                                $('loading').show(); 
                                $('<%=$this->btnClear->ClientId%>').disabled='disabled';						
                            </prop:ClientSide.OnPreDispatch>
                            <prop:ClientSide.OnLoading>
                                $('<%=$this->btnClear->ClientId%>').disabled='disabled';						
                            </prop:ClientSide.OnLoading>
                            <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                $('<%=$this->btnClear->ClientId%>').disabled='';
                                $('loading').hide(); 
                            </prop:ClientSide.OnComplete>
                        </com:TActiveLinkButton>
                    </div>
                </header>
                <div class="main-box-body clearfix">
                    <com:TActiveRepeater ID="RepeaterCart" DataKeyField="idcart" OnItemCreated="itemCreatedCart">
                        <prop:HeaderTemplate>			
                            <table class="table table-striped table-hover">	
                                <thead>
                                <tr>
                                    <th width="240"><span>NAMA OBAT</span></th>                                        
                                    <th width="90"><span>KEMASAN</span></th>                                                                            
                                    <th width="70">HARGA</th>
                                    <th width="90"><span>QTY</span></th>
                                    <th width="330"><span>TGL. EXPIRE</span></th>
                                    <th width="120"><span>NO.BATCH</span></th>
                                    <th width="100px"></th>							
                                </tr>									
                                </thead>
                                <tbody>
                        </prop:HeaderTemplate>
                        <prop:ItemTemplate>
                            <tr>
                                <td><%#$this->DataItem['nama_obat']%></td>                                   
                                <td><%#$this->DataItem['kemasan']%></td>
                                <td><%#$this->Page->Obat->toRupiah($this->DataItem['harga'])%></td>
                                <td>
                                    <com:TTextBox ID="txtQTY" CssClass="form-control" Attributes.OnKeyUp="formatangka(this,true)" Text="<%#$this->DataItem['qty']%>" Width="60px" />
                                    <com:TRequiredFieldValidator ValidationGroup="addsbbm" Display="Dynamic" ControlToValidate="txtQTY" ErrorMessage="Isi QTY Obat." FocusOnError="true" CssClass="has-error help-block" />	                                        
                                </td>
                                <td><com:TCustomDatePicker ID="cmbAddExpire" DateFormat="dd-MMMM-yyyy" Culture="id"  FromYear="<%#date('Y')%>" UpToYear="<%#date('Y')+10%>" InputMode="DropDownList" ShowCalendar="false" /></td>
                                <td>
                                    <com:TTextBox ID="txtNoBatch" CssClass="form-control" Width="120px" Text="<%#$this->DataItem['no_batch']%>" />
                                    <com:TRequiredFieldValidator ValidationGroup="addsbbm" Display="Dynamic" ControlToValidate="txtNoBatch" ErrorMessage="Isi No Batch." FocusOnError="true" CssClass="has-error help-block" />	                                        
                                </td>
                                <td width="100">                                       
                                    <com:TActiveLinkButton ID="btnAddProductRepeater" CssClass="table-link" OnClick="Page.saveItem" Attributes.Title="simpan">
                                        <prop:Text>
                                            <span class="fa-stack">
                                                <i class="fa fa-square fa-stack-2x"></i>
                                                <i class="fa fa-save fa-stack-1x fa-inverse"></i>
                                            </span>
                                        </prop:Text>
                                        <prop:ClientSide.OnPreDispatch>
                                            $('loading').show();                                             
                                            $('<%=$this->btnAddProductRepeater->ClientId%>').disabled='disabled';						
                                        </prop:ClientSide.OnPreDispatch>
                                        <prop:ClientSide.OnLoading>
                                            $('<%=$this->btnAddProductRepeater->ClientId%>').disabled='disabled';									                            
                                        </prop:ClientSide.OnLoading>
                                        <prop:ClientSide.onComplete>
                                            $('loading').hide();                                                
                                        </prop:ClientSide.OnComplete>
                                    </com:TActiveLinkButton>        
                                    <com:TActiveLinkButton ID="btnDelete" ClientSide.PostState="false" CssClass="table-link danger" OnClick="Page.deleteItem" Attributes.Title="Hapus Item" Attributes.OnClick="if(!confirm('Anda ingin menghapus item ini ?')) return false;">
                                        <prop:Text>
                                            <span class="fa-stack">
                                                <i class="fa fa-square fa-stack-2x"></i>
                                                <i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
                                            </span>
                                        </prop:Text>
                                        <prop:ClientSide.OnPreDispatch>
                                            $('loading').show();                                             
                                            $('<%=$this->btnDelete->ClientId%>').disabled='disabled';						
                                        </prop:ClientSide.OnPreDispatch>
                                        <prop:ClientSide.OnLoading>
                                            $('<%=$this->btnDelete->ClientId%>').disabled='disabled';									                            
                                        </prop:ClientSide.OnLoading>
                                        <prop:ClientSide.onComplete>
                                            $('loading').hide();                                                
                                        </prop:ClientSide.OnComplete>
                                    </com:TActiveLinkButton>  
