<%@ Title="Mutasi Barang Masuk - SBBM" %>
<com:TContent ID="moduleheader">
    <i class="fa fa-file"></i> SBBM (Surat Bukti Barang Masuk)
</com:TContent>
<com:TContent ID="modulebreadcrumb">    
    <li><a href="#">Mutasi Barang Masuk</a></li>
    <li class="active">SBBM</li>
</com:TContent>
<com:TContent ID="csscontent">
<link rel="stylesheet" href="<%=$this->Theme->baseUrl%>/css/libs/select2.css" type="text/css" />
<link rel="stylesheet" type="text/css" href="<%=$this->Theme->baseUrl%>/css/libs/nifty-component.css"/>
</com:TContent>
<com:TContent ID="maincontent">
<com:TActiveHiddenField ID="hiddenidobat" Visible="false" />
<com:THiddenField ID="hiddenno_faktur" />
<com:TPanel Visible="<%= $this->getEditProcess(false) %>" CssClass="row">
    <div class="col-lg-12">
        <div class="main-box clearfix">
            <header class="main-box-header clearfix">
                <h2 class="pull-left">No. SBBM : <%=$this->datasbbm['no_sbbm']%></h2>
                <div class="filter-block pull-right">                                                
                    <com:TActiveLinkButton ID="btnCloseEdit" CssClass="btn btn-warning pull-right" OnClick="closeSBBM" ValidationGroup="formNomorSBBM2">
                        <prop:Text><i class="fa fa-times fa-lg"></i></prop:Text>
                        <prop:ClientSide.OnPreDispatch>                                                                   
                            $('loading').show(); 
                            $('<%=$this->btnCloseEdit->ClientId%>').disabled='disabled';						
                        </prop:ClientSide.OnPreDispatch>
                        <prop:ClientSide.OnLoading>
                            $('<%=$this->btnCloseEdit->ClientId%>').disabled='disabled';						
                        </prop:ClientSide.OnLoading>
                        <prop:ClientSide.OnComplete>																	                                    						                                                                            
                            $('<%=$this->btnCloseEdit->ClientId%>').disabled='';
                            $('loading').hide(); 
                        </prop:ClientSide.OnComplete>
                    </com:TActiveLinkButton>
                </div>
            </header>
            <div class="main-box-body clearfix">
                <div class="form-horizontal">                                                        
                    <div class="form-group">
                        <label class="col-lg-2 control-label">Tanggal SBBM :</label>
                        <div class="col-lg-10">
                            <com:TCustomDatePicker ID="cmbEditTanggalSBBM" DateFormat="dd-MMMM-yyyy" Culture="id"  FromYear="<%=$_SESSION['ta']%>" UpToYear="<%=$_SESSION['ta']%>" InputMode="DropDownList" />
                        </div>
                    </div>                    
                    <div class="form-group form-group-select2" id="divEditPenyalur">
                        <label class="col-lg-2 control-label">Penyalur :</label>
                        <div class="col-lg-4">
                            <com:TDropDownList ID="cmbEditPenyalur" Width="300px" />
                            <com:TRequiredFieldValidator ID="FieldEditPenyalurValidator" InitialValue="none" ValidationGroup="editsbbm" Display="Dynamic" ControlToValidate="cmbEditPenyalur" ErrorMessage="Pilih Bentuk Sediaan Obat" CssClass="has-error help-block">	
                                <prop:ClientSide.OnValidationError>
                                    $('divEditPenyalur').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditPenyalurValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divEditPenyalur').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>    
                        </div>
                    </div>                                                
                    <div class="form-group" id="divEditNoFaktur">
                        <label class="col-lg-2 control-label">No. Faktur :</label>
                        <div class="col-lg-10">
                            <div class="row">
                                <div class="col-lg-3 col-sm-6">
                                    <com:TTextBox ID="txtEditNoFaktur" CssClass="form-control" /> 
                                </div>
                                <div class="col-lg-4 col-sm-6">
                                    <com:TRequiredFieldValidator ID="FieldEditNoFakturValidator" ValidationGroup="editsbbm" Display="Dynamic" ControlToValidate="txtEditNoFaktur" ErrorMessage="Isi No. Faktur" CssClass="has-error help-block">	
                                        <prop:ClientSide.OnValidationError>
                                            $('divEditNoFaktur').addClassName('has-error');
                                            jQuery('#<%=$this->FieldEditNoFakturValidator->ClientID%>').removeAttr('style');
                                        </prop:ClientSide.OnValidationError>
                                        <prop:ClientSide.OnValidationSuccess>
                                            $('divEditNoFaktur').removeClassName('has-error');
                                        </prop:ClientSide.OnValidationSuccess>
                                    </com:TRequiredFieldValidator>     
                                    <com:TActiveCustomValidator ID="editNoFaktur" ControlToValidate="txtEditNoFaktur" Display="Dynamic" OnServerValidate="checkNoFaktur" ValidationGroup="editsbbm" ErrorMessage="Nomor Faktur Yang Anda Inputkan Sudah Ada" FocusOnError="true" CssClass="has-error help-block">
                                        <prop:ClientSide.OnValidationError>
                                            $('divEditNoFaktur').addClassName('has-error');
                                            jQuery('#<%=$this->addNoFaktur->ClientID%>').removeAttr('style');
                                        </prop:ClientSide.OnValidationError>
                                        <prop:ClientSide.OnValidationSuccess>
                                            $('divEditNoFaktur').removeClassName('has-error');
                                        </prop:ClientSide.OnValidationSuccess>
                                    </com:TActiveCustomValidator>
                                </div>
                            </div>
                        </div>
                    </div> 
                    <div class="form-group">
                        <label class="col-lg-2 control-label">Tanggal Faktur :</label>
                        <div class="col-lg-10">
                            <com:TCustomDatePicker ID="cmbEditTanggalFaktur" DateFormat="dd-MMMM-yyyy" Culture="id"  FromYear="<%=$_SESSION['ta']%>" InputMode="DropDownList" />
                        </div>
                    </div>
                    <div class="form-group" id="divEditPenerima">
                        <label class="col-lg-2 control-label">Penerima :</label>
                        <div class="col-lg-10">
                            <div class="row">
                                <div class="col-lg-6 col-sm-6">
                                    <com:TTextBox ID="txtEditPenerima" CssClass="form-control" /> 
                                </div>
                                <div class="col-lg-4 col-sm-6">
                                    <com:TRequiredFieldValidator ID="FieldEditPenerimaValidator" ValidationGroup="editsbbm" Display="Dynamic" ControlToValidate="txtEditPenerima" ErrorMessage="Isi Penerima" CssClass="has-error help-block">	
                                        <prop:ClientSide.OnValidationError>
                                            $('divEditPenerima').addClassName('has-error');
                                            jQuery('#<%=$this->FieldEditPenerimaValidator->ClientID%>').removeAttr('style');
                                        </prop:ClientSide.OnValidationError>
                                        <prop:ClientSide.OnValidationSuccess>
                                            $('divEditPenerima').removeClassName('has-error');
                                        </prop:ClientSide.OnValidationSuccess>
                                    </com:TRequiredFieldValidator>     
                                </div>
                            </div>
                        </div>
                    </div>                       
                    <div class="form-group">
                        <div class="col-lg-offset-2 col-lg-10">
                            <com:TActiveCustomButton ID="btnUpdate" CssClass="btn btn-primary" OnClick="updateData" ValidationGroup="editsbbm">
                                <prop:Text><i class="fa fa-save"></i> Simpan</prop:Text>
                                <prop:ClientSide.OnPreDispatch>                                                                   
                                    $('loading').show(); 
                                    $('<%=$this->btnUpdate->ClientId%>').disabled='disabled';						                                    
                                </prop:ClientSide.OnPreDispatch>
                                <prop:ClientSide.OnLoading>
                                    $('<%=$this->btnUpdate->ClientId%>').disabled='disabled';						                                    
                                </prop:ClientSide.OnLoading>
                                <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                    $('<%=$this->btnUpdate->ClientId%>').disabled='';                                    
                                    $('loading').hide(); 
                                </prop:ClientSide.OnComplete>
                            </com:TActiveCustomButton>                            
                        </div>
                    </div>                    
                </div>                
            </div>
        </div>
    </div>
</com:TPanel>
<com:TPanel Visible="<%= $this->getAddProcess(false) %>">    
    <div class="row">
        <div class="col-lg-12">
